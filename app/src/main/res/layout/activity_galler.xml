<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/best"
    android:orientation="vertical"
    tools:context="com.example.nikhil.dronpreneur.Galler">


    <Button
        android:id="@+id/but2"
        android:layout_width="155sp"
        android:layout_height="50sp"
        android:layout_alignStart="@+id/but"
        android:layout_below="@+id/but"
        android:layout_marginTop="23dp"
        android:background="@drawable/circle"
        android:text="@string/_2016_2017"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="UnknownId" />

    <Button
        android:id="@+id/but"
        android:layout_width="155sp"
        android:layout_height="50sp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="142dp"
        android:background="@drawable/circle"
        android:text="@string/_2018"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        android:textStyle="bold" />


</RelativeLayout>
